<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="productUrl">https://mozib.io/amdownloader</system:String>
    <system:String x:Key="resetSettingsMsg">The application needs to be restarted for the changes to take effect.</system:String>
    <system:String x:Key="addDownloadHelpTitle">How to use URL patterns</system:String>
    <system:String x:Key="addDownloadHelpText"
                   xml:space="preserve">Patterns can be applied to download multiple files from a single URL. 
        For example, entering the following pattern:&#x0a;&#x0a;&#x09;http://www.example.com/file[1:10].png&#x0a;&#x0a;
        will download the following ten files: &#x0a;&#x0a;&#x09;file1.png&#x0a;&#x09;file2.png&#x0a;&#x09;
        ...&#x0a;&#x0a;&#x09;file10.png</system:String>
    <system:String x:Key="noUpdateMsg">No new updates are available.</system:String>
    <system:String x:Key="erroredUpdateMsg">An error occurred while trying to check for updates.</system:String>
</ResourceDictionary>